{
  "name": "offline-package-server",
  "version": "0.2.0",
  "description": "A offline package server",
  "author": "mcuking",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "export NODE_ENV=development && nest start",
    "start:dev": "export NODE_ENV=development && nest start --watch",
    "start:debug": "export NODE_ENV=development && nest start --debug --watch",
    "start:prod": "npm run build && node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.2.0",
    "bsdiff-nodejs": "^2.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "md5": "^2.2.1",
    "muter": "^0.7.0",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/md5": "^2.1.33",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.7.5",
    "@types/rimraf": "^2.0.3",
    "@types/supertest": "^2.0.8",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
